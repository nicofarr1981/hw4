<h1>Login</h1>

<%= form_with :url => "/sessions" do |form| %>
  <p>
    <%= form.label "email" %>
    <%= form.text_field "email" %>
    <%= form.label "password" %>
    <%= form.password_field "password" %>
  </p>
  <p>
    <button class="btn btn-secondary" type="submit">Login</button>
  </p>
<% end %>